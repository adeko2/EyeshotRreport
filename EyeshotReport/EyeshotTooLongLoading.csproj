<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="devDept.Eyeshot" Version="2024.2.342" />
    <PackageReference Include="devDept.Eyeshot.Control.Wpf" Version="2024.2.342" />
    <PackageReference Include="devDept.Eyeshot.x64" Version="2024.2.342" />
  </ItemGroup>

  <ItemGroup>
    <None Update="oven.pbf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
